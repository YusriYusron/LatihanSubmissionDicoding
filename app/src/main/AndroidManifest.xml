<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yusriyusron.volume">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intent.IntentActivity"/>
        <activity android:name=".intent.data.IntentDataActivity" />
        <activity android:name=".intent.object.IntentObjectActivity" />
        <activity android:name=".intent.hasil.IntentHasilActivity" />
        <activity android:name=".volume.VolumeActivity" />
        <activity android:name=".fragment.MainFragment" />
        <activity android:name=".view.ViewViewGroupActivity" />
        <activity android:name=".recycler_view.RecyclerViewActivity" />
        <activity android:name=".fragment.ProfileActivity" />
    </application>

</manifest>